;
; BIND data file for local loopback interface
;
$TTL	604800
@	IN	SOA	diac.ifrn.asa.br. root.diac.asa.br. (
			      3		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
;
@	IN  MX 10 mail.diac.ifrn.asa.br.
@	IN	NS	ns1.diac.ifrn.asa.br.

ns1			A	192.168.1.10 ;10.25.2.30 
mail		A 	192.168.1.10 ;10.25.2.30 	
www			CNAME	ns1